# ==== Discrete LEDs (LED) ====
# These are shared connections with the FX2 connector
NET "q" LOC = "F12" | IOSTANDARD = LVTTL | SLEW = SLOW | DRIVE = 8 ;
NET "qn" LOC = "E12" | IOSTANDARD = LVTTL | SLEW = SLOW | DRIVE = 8 ;

# ==== Slide Switches (SW) ====
NET "d" LOC = "L13" | IOSTANDARD = LVTTL | PULLUP ;
NET "set" LOC = "L14" | IOSTANDARD = LVTTL | PULLUP ;
NET "reset" LOC = "H18" | IOSTANDARD = LVTTL | PULLUP ;

NET "clk" LOC = "C9" | IOSTANDARD = LVCMOS33 ;